{
    "name": "wickedbyte/project-skeleton",
    "description": "Template for PHP Projects",
    "type": "project",
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "phpbench/phpbench": "^1.2",
        "phpstan/phpstan": "^1",
        "phpunit/phpunit": "^10",
        "psy/psysh": "^0.11",
        "squizlabs/php_codesniffer": "^3",
        "symfony/console": "^6",
        "slevomat/coding-standard": "^8.10",
        "rector/rector": "^0.15.24"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "WickedByte\\Foo\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WickedByte\\Benchmarks\\Foo\\": "benchmarks/",
            "WickedByte\\Tests\\Foo\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Andy Snell",
            "email": "andy@wickedbyte.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    }
}
